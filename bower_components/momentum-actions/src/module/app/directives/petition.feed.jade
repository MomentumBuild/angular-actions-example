#feed
	#empty(ng-if="!Feed.actions.length")
		p No one has currently taking any actions on this page...
	.actionItem(ng-if="Feed.actions.length" ng-repeat="a in Feed.actions | orderBy:'-createdAt'")
		p <b>{{ a.first_name }} said</b> {{ a.message}}
			small.clearfix at {{ a.createdAt | date : 'medium' }}

